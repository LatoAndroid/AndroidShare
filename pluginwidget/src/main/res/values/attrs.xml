<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="XSwipeRefreshLayout">
        <attr name="swipeMode" format="enum">
            <enum name="TOP_ONLY" value="1" />
            <enum name="BOTTOM_ONLY" value="2" />
            <enum name="BOTH" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="PullRefreshView">
        <attr name="state_normal_str" format="string" />
        <attr name="state_ready_str" format="string" />
        <attr name="state_loading_str" format="string" />
        <attr name="pull_arrow" format="reference" />
        <attr name="pull_class" format="string" />
    </declare-styleable>
    <declare-styleable name="BaseTextView">

    </declare-styleable>
</resources>